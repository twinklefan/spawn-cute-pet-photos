{
  "icons": {
    "16": "./img/icon-2-16.png",
    "32": "./img/icon-2-32.png",
    "128": "./img/icon-2-128.png"
},
  "name": "Cute Pets!",
  "version": "2.2",
  "manifest_version": 2,
  "description": "A extension to place cute pet photos!",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "CUTENESS :)"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}