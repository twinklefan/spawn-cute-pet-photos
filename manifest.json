{
  "icons": {
    "16": "./img/icon-16.png",
    "32": "./img/icon-32.png",
    "128": "./img/icon-128.png"
},
  "name": "Cute Pets!",
  "version": "2.5",
  "manifest_version": 2,
  "description": "A extension to place cute pet photos!",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "CUTENESS :)"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ],
  "_execute_action": {
    "suggested_key": {
      "windows": "Ctrl+Shift+Z",
      "mac": "Command+Shift+Z",
      "chromeos": "Ctrl+Shift+Z",
      "linux": "Ctrl+Shift+Z"
    }
  }
}